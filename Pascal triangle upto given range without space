n=int(input())
for i in range(n):
    a=int(input())
    for j in range(0,a):
        x=1
        for k in range(0,j+1):
            print(x,end=" ")
            x=x*(j-k)//(k+1)
        print()

'''
TSET CASE 1:
2
2
4

OUTPUT:

1 
1 1

1 
1 1 
1 2 1 
1 3 3 1

TSET CASE 2:
1
5

OUTPUT:
1 
1 1 
1 2 1 
1 3 3 1
1 4 6 4 1
'''
